<!DOCTYPE html>
<html>
<head>
  <title>Frankenapp</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>
    <p>Signed in as: <%= current_user.email %></p>
    <ul>
      <li class="blog-nav-item"><%= link_to 'Log out', '/users/sign_out', method: :delete %></li>
    </ul>
<% else %>
    <ul class="blog-nav">
      <li class="blog-nav-item"><a href="/users/sign_up">Sign Up</a></li>
      <li class="blog-nav-item"><a href="/users/sign_in">Log In</a></li>
    </ul>
<% end %>

<%= yield %>

</body>
</html>
